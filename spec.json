{
    "openapi": "3.0.0",
    "info": {
        "title": "GitHub Markdown Diff Aggregator",
        "version": "0.1.0"
    },
    "paths": {
        "/get-latest-updates": {
            "post": {
                "summary": "Get latest changes commits updates from a github repository",
                "parameters": [
                    {
                        "name": "repo",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "date",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Aggregated diffs",
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}